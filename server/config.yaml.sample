postgresConnString: "host=postgres user=postgres password=postgres port=5432 sslmode=disable"
jaegerConnectionConfig:
  endpoint: jaeger-query:16685
  tls:
    insecure: true

maxWaitTimeForTrace: 30s

googleAnalytics:
  enabled: false
  measurementId: ""
  secretKey: ""

telemetry:
  serviceName: tracetest
  sampling: 100 # 100%
  jaeger:
    host: localhost
    port: 6831
  exporters:
    - console
    - jaeger